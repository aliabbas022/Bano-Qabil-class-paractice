import math

a = math.sqrt(49)
b = math.ceil(2.8)
c = math.floor(2.8)
d = math.pi
import datetime
e = datetime.datetime.now()

print(a)
print(b)
print(c)
print(d)
print(e.year)
print(e.strftime("%A"))

#Declaring a function
